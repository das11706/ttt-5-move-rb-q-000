#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go? Choose a position from 1-9."
position = gets.strip
puts "Choose char X or O."
char = gets.strip
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
move(board, position, char="X")
